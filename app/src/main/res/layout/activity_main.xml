<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    >

    <ImageView
        android:id="@+id/brick1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_gray"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toEndOf="@+id/brick1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_pink"
        tools:ignore="ImageContrastCheck,ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toEndOf="@+id/brick2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_purple"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toEndOf="@+id/brick3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_pink"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_red"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="48dp"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_green"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="252dp"
        android:layout_marginTop="48dp"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_gray"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/brick8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="96dp"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_green" />

    <ImageView
        android:id="@+id/brick9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="204dp"
        android:layout_marginTop="96dp"
        android:contentDescription="@string/brick1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/brick_purple" />

    <ImageView
        android:id="@+id/ball"
        android:layout_width="100dp"
        android:layout_height="63dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="108dp"
        android:contentDescription="@string/ball"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ball"
        tools:ignore="ImageContrastCheck" />



    <TextView
        android:id="@+id/test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>